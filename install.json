  "python_version": "3.10+",
  "dependencies": [
    "torch>=2.0.0",
    "numpy>=1.24.0",
    "pillow>=10.0.0",
    "requests>=2.31.0",
    "scipy>=1.7.0",
    "scikit-learn>=1.0.0",
    "torchaudio>=0.9.0",
    "librosa>=0.9.0",
    "soundfile>=0.10.0",
    "python-dotenv>=1.0.0"
  ],
  "optional_dependencies": [
    "speechbrain>=0.5.0"
  ],
  "requirements_path": "requirements.txt",
  "minimum_comfyui_version": "0.1.0",
  "tags": [
    "AI绘画",
    "提示词优化", 
    "图像分析",
    "音频处理",
    "人声分离",
